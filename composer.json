{
    "name": "growthexponent/laravel-html-dom-parser",
    "description": "Laravel wrapper for the PHP HTML DOM Parser package.",
    "keywords": ["laravel html dom parser", "laravel", "html", "dom", "parser"],
    "license": "MIT",
    "authors": [
        {
            "name": "jpascoe",
            "email": "jpascoe@growthexponent.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "laravel/framework": "5.1.*",
        "paquettg/php-html-parser": "1.6.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelHtmlDomParser\\": "src"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
